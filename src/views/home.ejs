<html lang="en">

<head>
	<%- include('./partials/head') %>
		<title>Return Soon</title>
</head>

<body>
	<%- include('./partials/header') %>

		<main class="container mt-5">

			<div class="block-wrapper">
				<div class="row">
					<div class="col-12 col-md-4 mb-2">
						<div class="card h-100 p-3 " style="min-height: 200px;">
							<div class="card-body d-flex align-items-center justify-content-between">
								<h5>Total Responses</h5>
								<h2 class="mb-0">
									<%= allCount %>
								</h2>
							</div>
						</div>
					</div>
					<div class="col-12 col-md-4 mb-2">
						<div class="card h-100 p-3 " style="min-height: 200px;">
							<div class="card-body d-flex align-items-center justify-content-between">
								<h5>Paid Responses</h5>
								<h2 class="mb-0">
									<%= paidCount %>
								</h2>
							</div>
							<a href="/page/paid" class="btn btn-warning">View</a>
						</div>
					</div>
					<div class="col-12 col-md-4 mb-2">
						<div class="card h-100 p-3 " style="min-height: 200px;">
							<div class="card-body d-flex align-items-center justify-content-between">
								<h5>Unpaid Responses</h5>
								<h2 class="mb-0">
									<%= unPaidCount %>
								</h2>
							</div>
							<a href="/page/unpaid" class="btn btn-warning">View</a>
						</div>
					</div>
				</div>
			</div>

		</main>


		<%- include('./partials/scripts') %>
</body>

</html>